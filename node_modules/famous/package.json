{
  "name": "famous",
  "version": "0.3.0",
  "description": "Famo.us is a JavaScript framework for everyone who wants to build beautiful experiences on any device.",
  "repository": {
    "type": "git",
    "url": "https://github.com/famous/famous"
  },
  "keywords": [
    "famous"
  ],
  "scripts": {
    "test": "npm run lint -s",
    "lint-eslint": "eslint src/",
    "lint-jscs": "jscs src/",
    "lint": "npm run lint-eslint && npm run lint-jscs",
    "build": "npm run build-requirejs && npm run build-requirejs-min && npm run build-global && npm run build-global-min && npm run build-css",
    "build-requirejs": "famous-dist-generator --requirejs --local --out dist/famous.js",
    "build-requirejs-min": "famous-dist-generator --requirejs --local --minify --out dist/famous.min.js",
    "build-global": "famous-dist-generator --standalone --local --out dist/famous-global.js",
    "build-global-min": "famous-dist-generator --standalone --local --out dist/famous-global.min.js",
    "build-css": "famous-dist-generator --css --local --out dist/famous.css",
    "build-docs": "famous-doc-generator --base src/ --out docs/ --pathPrefix ../",
    "build-examples": "famous-examples-generator -i ./examples"
  },
