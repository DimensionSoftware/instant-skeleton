{
  "name": "instant-skeleton",
  "version": "3.2.1",
  "description": "The Best, Most Opinionated Realtime Framework to Lift Heavy Functionality Lightening Quick with Node.JS",
  "main": "build/server/main.js",
  "engines": {
    "node": ">= 4.2.2"
  },
  "scripts": {
    "pretest": "node --harmony `which gulp` build:test",
    "test": "NODE_ENV=test npm run unit",
    "docs": "groc --github ![node_modules/] server/main.ls {server,shared,client}/**.ls README.md",
    "start": "bin/start",
    "stop": "bin/stop",
    "restart": "bin/restart",
    "unit": "mocha -R nyan --harmony --compilers ls:livescript",
    "coverage": "mocha -r blanket -R html-cov --harmony --compilers ls:livescript > build/coverage.html && open build/coverage.html",
    "clean": "node --harmony `which gulp` clean"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DimensionSoftware/instant-skeleton.git"
  },
  "keywords": [
    "Dimension",
    "Software",
    "Instant",
    "Skeleton",
    "Best",
    "Most",
    "Opinionated",
    "Realtime",
    "Framework",
    "Lift",
    "Heavy",
    "Functionality",
    "Lightening",
    "Quick",
    "Node.JS",
    "Meteor",
    "Derby",
    "JS"
  ],
  "author": "Dimension Software <keith@dimensionsoftware.com> (https://dimensionsoftware.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimensionSoftware/instant-skeleton/issues"
  },
  "homepage": "https://github.com/DimensionSoftware/instant-skeleton",
  "config": {
    "node_port": 8080,
    "dev_port": 8081,
    "rethinkdb_port": 28015,
    "title": "Instant Skeleton | by DimensionSoftware.com",
    "domain": "develop.com",
    "cache_url": "//cache%n.%domain",
    "development": {
      "limits": {
        "max": 0,
        "whiteList": [
          "::ffff:127.0.0.1"
        ]
      }
    },
    "test": {},
    "production": {
      "domain": "dimensionsoftware.com",
      "cacheUrl": "//cache%n.%domain",
      "limits": {
        "whiteList": [],
        "blackList": [],
        "duration": 1800000,
        "max": 800
      }
    },
    "blanket": {
      "pattern": [
        "build/server/main.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "classnames": "^2.2.2",
    "co": "^4.6.0",
    "css-loader": "^0.23.1",
    "dotenv": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "immstruct": "^2.0.0",
    "immutable": "^3.7.6",
    "keygrip": "^1.0.1",
    "koa": "^1.1.2",
    "koa-better-ratelimit": "^2.1.2",
    "koa-generic-session": "^1.10.1",
    "koa-generic-session-rethinkdb": "^1.1.1",
    "koa-helmet": "^1.0.0",
    "koa-jade": "^2.0.2",
    "koa-locals": "^0.3.0",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.3.0",
    "koa-static-cache": "^3.1.4",
    "livescript": "^1.4.0",
    "livescript-loader": "^0.1.5",
    "moment": "^2.11.0",
    "nib": "~1.1.0",
    "omnipotent": "^1.2.1",
    "omniscient": "^4.1.1",
    "pm2": "^1.0.0",
    "prelude-ls": "^1.1.2",
    "pretty-error": "^2.0.0",
    "react": "^0.14.5",
    "react-async": "DimensionSoftware/react-async#react-0.14.x",
    "react-dom": "^0.14.5",
    "react-rethinkdb": "^0.5.1",
    "react-router-component": "^0.29.0",
    "replacestream": "^4.0.0",
    "rethinkdb-websocket-server": "^0.3.3",
    "rethinkdbdash": "^2.2.15",
    "shelljs": "^0.5.3",
    "style-loader": "^0.13.0",
    "stylus": "^0.53.0",
    "stylus-loader": "^1.4.2",
    "superagent": "^1.6.1",
    "tls": "0.0.1",
    "uuid": "^2.0.1",
    "webpack": "^1.12.9",
    "webpack-sources": "^0.1.0",
    "ws": "^1.0.1"
  },
  "devDependencies": {
    "blanket": "^1.2.1",
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-livescript": "^3.0.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-shell": "^0.5.1",
    "gulp-stylus": "^2.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "webpack-stream": "^3.1.0",
    "mocha": "^2.3.4",
    "open": "0.0.5",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0",
    "webpack-dev-server": "^1.14.0"
  }
}
