{
  "name": "instant-skeleton",
  "version": "0.0.1",
  "description": "The Best, Most Opinionated Realtime Framework to Lift Heavy Functionality Lightening Quick with Node.JS",
  "main": "build/server/main.js",
  "config": {
    "blanket": {
      "pattern": "![node_modules] build/**.js",
      "data-cover-never": "node_modules"
    }
  },
  "engines": {
    "node": ">= 0.11.14"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run unit",
    "docs": "groc --github ![node_modules/] server/main.ls {server,shared,client}/**.ls README.md",
    "start": "node --harmony `which gulp`",
    "stop": "gulp stop",
    "unit": "mocha -R nyan --harmony",
    "coverage": "mocha -r blanket -R html-cov --harmony > coverage.html && open coverage.html"
  },
  "browser": {
    "fs": false
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DimensionSoftware/instant-skeleton.git"
  },
  "keywords": [
    "Dimension",
    "Software",
    "Instant",
    "Skeleton",
    "Best",
    "Most",
    "Opinionated",
    "Realtime",
    "Framework",
    "Lift",
    "Heavy",
    "Functionality",
    "Lightening",
    "Quick",
    "Node.JS",
    "Meteor",
    "Derby",
    "JS"
  ],
  "author": "Dimension Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimensionSoftware/instant-skeleton/issues"
  },
  "homepage": "https://github.com/DimensionSoftware/instant-skeleton",
  "dependencies": {
    "LiveScript": "^1.3.1",
    "accounting": "^0.4.1",
    "async": "^0.9.0",
    "bluebird": "^2.3.4",
    "body-parser": "^1.9.0",
    "co": "^3.1.0",
    "co-body": "^1.0.0",
    "del": "^0.1.3",
    "engine.io": "^1.4.2",
    "famous": "^0.3.0",
    "gulp": "^3.8.8",
    "gulp-jade": "^0.8.0",
    "gulp-livereload": "^2.1.1",
    "gulp-livescript": "^1.1.0",
    "gulp-nodemon": "^1.0.4",
    "gulp-shell": "^0.2.10",
    "gulp-spawn": "^0.3.0",
    "gulp-stylus": "^1.3.3",
    "gulp-watch": "^1.0.7",
    "gulp-webpack": "^0.4.1",
    "jade": "^1.7.0",
    "jquery": "^2.1.1",
    "koa": "^0.12.2",
    "koa-jade": "^0.3.1",
    "koa-livereload": "^0.1.23",
    "koa-locals": "^0.3.0",
    "koa-logger": "^1.2.2",
    "koa-router": "^3.4.0",
    "koa-static": "^1.4.7",
    "leveldown": "^1.0.0",
    "levelup": "^0.19.0",
    "lodash": "^2.4.1",
    "moment": "^2.8.3",
    "multilevel": "^6.0.1",
    "nib": "^1.0.4",
    "nodemon": "^1.2.1",
    "pm2": "^0.10.8",
    "prelude-ls": "^1.1.1",
    "pretty-error": "^1.1.1",
    "primus": "^2.4.10",
    "primus-emitter": "^3.0.3",
    "react": "^0.11.2",
    "react-responsive": "0.0.1",
    "react-router": "^0.9.4",
    "redis": "^0.12.1",
    "replacestream": "^0.3.0",
    "shelljs": "^0.3.0",
    "stylus": "^0.49.1",
    "webpack": "^1.4.13"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.2",
    "mocha": "^1.21.5",
    "sinon": "^1.10.3",
    "supertest": "^0.14.0"
  }
}
