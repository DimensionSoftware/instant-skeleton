{
  "name": "instant-skeleton",
  "version": "0.0.1",
  "description": "The Best, Most Opinionated Realtime Framework to Lift Heavy Functionality Lightening Quick with Node.JS",
  "main": "build/server/main.js",
  "engines": {
    "node": ">= 0.11.14"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run unit",
    "docs": "groc --github ![node_modules/] server/main.ls {server,shared,client}/**.ls README.md",
    "start": "node --harmony `which gulp`",
    "stop": "node --harmony `which gulp` stop",
    "unit": "mocha -R nyan --harmony",
    "coverage": "mocha -r blanket -R html-cov --harmony > build/coverage.html && open build/coverage.html",
    "clean": "node --harmony `which gulp` clean"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DimensionSoftware/instant-skeleton.git"
  },
  "keywords": [
    "Dimension",
    "Software",
    "Instant",
    "Skeleton",
    "Best",
    "Most",
    "Opinionated",
    "Realtime",
    "Framework",
    "Lift",
    "Heavy",
    "Functionality",
    "Lightening",
    "Quick",
    "Node.JS",
    "Meteor",
    "Derby",
    "JS"
  ],
  "author": "Dimension Software <hello@dimensionsoftware.com> (https://dimensionsoftware.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimensionSoftware/instant-skeleton/issues"
  },
  "homepage": "https://github.com/DimensionSoftware/instant-skeleton",

  "config": {
    "title": "Instant Skeleton | by DimensionSoftware.com",
    "url": "http://dimensionsoftware.com",
    "subdomain": "develop.com",
    "cacheUrls": [
      "//cache.develop.com",
      "//cache2.develop.com",
      "//cache3.develop.com",
      "//cache4.develop.com",
      "//cache5.develop.com"
    ],
    "development": {
      "limits": {
        "max": 0,
        "whiteList": ["::ffff:127.0.0.1"]
      }
    },
    "blanket": {
      "pattern": [
        "build/server/main.js"
      ],
      "data-cover-never": "node_modules"
    },
    "test": {
    },
    "production": {
      "cacheUrls": [
        "//muscache.dimensionsoftware.com",
        "//muscache2.dimensionsoftware.com",
        "//muscache3.dimensionsoftware.com",
        "//muscache4.dimensionsoftware.com",
        "//muscache5.dimensionsoftware.com"
      ],
      "limits": {
        "whiteList": [],
        "blackList": [],
        "duration": 1800000,
        "max": 800
      }
    }
  },

  "dependencies": {
    "LiveScript": "^1.3.1",
    "accounting": "^0.4.1",
    "async": "^0.9.0",
    "bluebird": "^2.3.4",
    "body-parser": "^1.9.0",
    "co": "^4.0.0",
    "co-body": "^1.0.0",
    "css-loader": "^0.9.0",
    "del": "^0.1.3",
    "dotenv": "^0.4.0",
    "engine.io": "^1.4.2",
    "extract-text-webpack-plugin": "^0.3.5",
    "famous": "^0.3.0",
    "geoip-lite": "^1.1.4",
    "gulp": "^3.8.10",
    "gulp-jade": "^0.8.0",
    "gulp-livescript": "^1.1.0",
    "gulp-nodemon": "^1.0.4",
    "gulp-shell": "^0.2.10",
    "gulp-spawn": "^0.3.0",
    "gulp-stylus": "^1.3.3",
    "gulp-watch": "^1.2.1",
    "gulp-webpack": "^0.4.1",
    "jade": "^1.7.0",
    "jquery": "^2.1.1",
    "koa": "^0.12.2",
    "koa-better-ratelimit": "^2.0.1",
    "koa-generic-session": "^1.3.0",
    "koa-helmet": "0.0.1",
    "koa-jade": "^0.3.1",
    "koa-level": "^1.0.0",
    "koa-locals": "^0.3.0",
    "koa-logger": "^1.2.2",
    "koa-router": "^3.4.0",
    "koa-static-cache": "^1.2.0",
    "level-sublevel": "^6.3.15",
    "leveldown": "^1.0.0",
    "levelup": "^0.19.0",
    "lodash": "^2.4.1",
    "moment": "^2.8.3",
    "multilevel": "^6.0.1",
    "nib": "^1.0.4",
    "open": "0.0.5",
    "pm2": "^0.10.8",
    "prelude-ls": "^1.1.1",
    "pretty-error": "^1.1.1",
    "primus": "^2.4.12",
    "primus-emitter": "^3.0.3",
    "react": "^0.12.1",
    "react-responsive": "0.0.1",
    "redis": "^0.12.1",
    "replacestream": "^0.3.0",
    "shelljs": "^0.3.0",
    "stylus": "^0.49.1",
    "stylus-loader": "^0.4.0",
    "substream": "^0.1.3",
    "webpack": "^1.4.13"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.2",
    "mocha": "^1.21.5",
    "sinon": "^1.10.3",
    "supertest": "^0.14.0"
  }
}
