{
  "name": "instant-skeleton",
  "version": "2.3.0",
  "description": "The Best, Most Opinionated Realtime Framework to Lift Heavy Functionality Lightening Quick with Node.JS",
  "main": "build/server/main.js",
  "engines": {
    "node": ">= 0.12.0",
    "iojs": ">= 1.3.0"
  },
  "scripts": {
    "pretest": "node --harmony `which gulp` build:test",
    "test": "NODE_ENV=test npm run unit",
    "docs": "groc --github ![node_modules/] server/main.ls {server,shared,client}/**.ls README.md",
    "start": "bin/start",
    "stop": "bin/stop",
    "unit": "mocha -R nyan --harmony --compilers ls:livescript",
    "coverage": "mocha -r blanket -R html-cov --harmony --compilers ls:livescript > build/coverage.html && open build/coverage.html",
    "clean": "node --harmony `which gulp` clean"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DimensionSoftware/instant-skeleton.git"
  },
  "keywords": [
    "Dimension",
    "Software",
    "Instant",
    "Skeleton",
    "Best",
    "Most",
    "Opinionated",
    "Realtime",
    "Framework",
    "Lift",
    "Heavy",
    "Functionality",
    "Lightening",
    "Quick",
    "Node.JS",
    "Meteor",
    "Derby",
    "JS"
  ],
  "author": "Dimension Software <keith@dimensionsoftware.com> (https://dimensionsoftware.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DimensionSoftware/instant-skeleton/issues"
  },
  "homepage": "https://github.com/DimensionSoftware/instant-skeleton",
  "config": {
    "node_port": 8080,
    "dev_port": 8081,
    "title": "Instant Skeleton | by DimensionSoftware.com",
    "domain": "develop.com",
    "cache_url": "//cache%n.%domain",
    "development": {
      "limits": {
        "max": 0,
        "whiteList": [
          "::ffff:127.0.0.1"
        ]
      }
    },
    "test": {},
    "production": {
      "domain": "dimensionsoftware.com",
      "cacheUrl": "//cache%n.%domain",
      "limits": {
        "whiteList": [],
        "blackList": [],
        "duration": 1800000,
        "max": 800
      }
    },
    "blanket": {
      "pattern": [
        "build/server/main.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "co": "^4.5.4",
    "css-loader": "^0.16.0",
    "dotenv": "^1.1.0",
    "engine.io": "^1.5.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "immstruct": "^2.0.0",
    "immutable": "^3.7.3",
    "keygrip": "^1.0.1",
    "koa": "^1.0.0",
    "koa-better-ratelimit": "^2.1.2",
    "koa-generic-session": "^1.8.0",
    "koa-helmet": "0.2.0",
    "koa-jade": "^1.0.2",
    "koa-level": "^1.0.0",
    "koa-locals": "^0.3.0",
    "koa-logger": "^1.2.2",
    "koa-router": "^5.1.1",
    "koa-static-cache": "^3.1.1",
    "level-live-stream": "^1.4.9",
    "level-party": "^2.1.1",
    "level-sublevel": "^6.4.6",
    "leveldown": "^1.0.5",
    "levelup": "^1.1.1",
    "livescript": "^1.4.0",
    "livescript-loader": "^0.1.5",
    "moment": "^2.10.3",
    "nib": "~1.1.0",
    "omniscient": "^3.1.0",
    "pm2": "^0.15.10",
    "prelude-ls": "^1.1.2",
    "pretty-error": "^1.1.2",
    "primus": "^3.0.2",
    "primus-emitter": "^3.1.0",
    "primus-multiplex": "^3.1.0",
    "primus-resource": "^2.0.6",
    "react": "^0.13.3",
    "react-async": "^2.1.0",
    "react-responsive": "0.0.8",
    "react-router-component": "^0.26.0",
    "replacestream": "^4.0.0",
    "shelljs": "^0.5.1",
    "style-loader": "^0.13.0",
    "stylus": "^0.51.1",
    "stylus-loader": "^1.2.0",
    "superagent": "^1.2.0",
    "uuid": "^2.0.1",
    "webpack": "^1.9.10"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^3.2.0",
    "gulp": "^3.9.0",
    "gulp-livescript": "^2.4.0",
    "gulp-nodemon": "^2.0.3",
    "gulp-shell": "^0.4.1",
    "gulp-stylus": "^2.0.3",
    "gulp-util": "^3.0.5",
    "gulp-watch": "^4.2.4",
    "webpack-stream": "^2.1.0",
    "mocha": "^2.2.5",
    "open": "0.0.5",
    "react-hot-loader": "^1.2.7",
    "sinon": "^1.14.1",
    "supertest": "^1.0.1",
    "webpack-dev-server": "^1.9.0"
  }
}
